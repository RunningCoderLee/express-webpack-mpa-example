{
  "name": "express-webpack-mpa-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean:client": "rm -rf static/dist server/views",
    "clean:server": "rm -rf server/dist",
    "build:client": "npm run 'clean:client' && webpack --config client/config/webpack.config.js",
    "build:server": "npm run 'clean:server' && babel server/src -d server/dist",
    "start:dev": "nodemon --exec babel-node server/src/index",
    "start:prod": "npm run 'build:client' && npm run 'build:server' && node server/dist/index"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "css-loader": "^0.28.7",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "ejs-lint": "^0.3.0",
    "express": "~4.15.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "morgan": "~1.9.0",
    "nodemon": "^1.12.1",
    "serve-favicon": "~2.4.5",
    "style-loader": "^0.19.0",
    "webpack": "^3.9.1"
  },
  "devDependencies": {
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0"
  }
}
